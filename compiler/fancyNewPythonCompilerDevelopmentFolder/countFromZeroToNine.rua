set:30:0
set:31:48
mode:1

::loop
    //print byte 31
        copy:31:6
    //print new line
        set:6:10
    //inc byte 30 and 31
        inc:30
        inc:31

    //test to see if byte 30 is equal to 10
        set:32:10
        sub:30:32:32
        ifResultZero:endLoop /jumping command
        goto:loop
::endLoop