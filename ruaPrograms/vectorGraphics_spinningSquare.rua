// this program creates a spinning red square
// (which spins forever, so you need to end the program yourself)

//create square
    inc:11
        set:10:3
    inc:11
        set:10:224
    inc:11
        set:10:127
    inc:11
        set:10:127
    inc:11
        set:10:50
    inc:11
        set:10:50
    inc:11
        set:10:10

//update square angle and loop
    ::loop
        inc:10
        wait:50
    goto:loop